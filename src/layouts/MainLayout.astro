--- 
import Footer from '../components/Footer.astro';
import Nav from '../components/Nav.astro';
import RootLayout from './RootLayout.astro';

interface Props {
	title: string;
	description?: string;
}

const { title, description } = Astro.props;
---

<RootLayout title={title} description={description}>
	<Nav />
	<main>
		<slot />
	</main>
	<Footer />
</RootLayout>